'program': "Simple loop map function Example .Hw (Append)";
function map [f, list] do
    list' as {};
    for element of list do
        list' append f [element]
    done;
    return list'
end;

function main [] do
    list as {1, 2, 3, 4, 5};
    square as (function[n] do return n * n end);
    echo [map [square, list]]
end; main[]
'program': "Simple loop map function Example .Hw (Verbose Syntax)";
function map [f, list] do
    for n as null to length list do
        list<<n>> is f [list<<n>>]
    done;
    return list
end;

function main [] do
    list as {1, 2, 3, 4, 5};
    square as (function[n] do return n * n end);
    echo [map [square, list]]
end; main[]